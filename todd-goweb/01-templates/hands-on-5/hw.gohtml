<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Hands-on 5</title>
</head>
<body>
<h1>STOCK DATA TEMPLATE</h1>
<table>
    <th>
        <td>Date</td><td>Open</td><td>High</td><td>Low</td><td>Close</td><td>Adjusted Close</td><td>Volume</td>
    </th>
    {{ range . }}
    <tr>
        <td>{{.Date}}</td><td>{{.Open}}</td><td>{{.High}}</td><td>{{.Low}}</td><td>{{.Close}}</td><td>{{.AdjClose}}</td><td>{{.Volume}}</td>
    </tr>
    {{ end }}
</table>

</body>
</html>